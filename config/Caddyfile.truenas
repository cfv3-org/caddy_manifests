{
    email admin+internal@cfv3.org
    debug
    auto_https off
}

http://*.cfv3.org {
    reverse_proxy {labels.2}:80

    handle_errors {
        respond "Service not available" 503
    }
}

http://:80 {
    header Content-Type application/json
    respond `{"error":"service not registered","host":"{http.request.host}"}` 404
}
